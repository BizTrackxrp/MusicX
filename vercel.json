{
  "buildCommand": "",
  "outputDirectory": ".",
  "framework": null,
  "rewrites": [
    { "source": "/api/ipfs/:cid/:path*", "destination": "/api/ipfs/[cid]?cid=:cid&path=:path*" },
    { "source": "/release/:id", "destination": "/api/og/release?id=:id" },
    { "source": "/artist/:address", "destination": "/api/og/artist?address=:address" },
    { "source": "/playlist/:id", "destination": "/app.html" },
    { "source": "/stream", "destination": "/app.html" },
    { "source": "/marketplace", "destination": "/app.html" },
    { "source": "/analytics", "destination": "/app.html" },
    { "source": "/profile", "destination": "/app.html" },
    { "source": "/liked", "destination": "/app.html" },
    { "source": "/playlists", "destination": "/app.html" },
    { "source": "/purchase", "destination": "/app.html" },
    { "source": "/genres", "destination": "/app.html" },
    { "source": "/genre/:id", "destination": "/app.html" }
  ],
  "functions": {
    "api/batch-mint.js": {
      "maxDuration": 300,
      "memory": 3008
    },
    "api/upload.js": {
      "maxDuration": 60,
      "memory": 3008
    }
  }
}
